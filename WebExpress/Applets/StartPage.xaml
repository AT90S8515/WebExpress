<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WebExpress"
             xmlns:Bookmarks="clr-namespace:WebExpress.Bookmarks" x:Class="WebExpress.StartPage"
             mc:Ignorable="d" 
             d:DesignHeight="400.5" d:DesignWidth="808.5">
    <Grid>
        <Grid x:Name="BookContainer" Height="372" Width="600" VerticalAlignment="Center" HorizontalAlignment="Stretch">
            <Label x:Name="label" Content="Bookmarks" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="21.333"/>

            <Bookmarks:Bookmarks x:Name="bookmarks" HorizontalAlignment="Stretch" Height="320" Width="550" Margin="15,53,15,0" VerticalAlignment="Top" Background="White"/>

        </Grid>
    </Grid>
</UserControl>
